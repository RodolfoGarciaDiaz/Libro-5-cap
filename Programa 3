#include <stdio.h>
/* eco de los argumentos de la línea de órdenes; 2a. versión */
main(int argc, char *argv[])
{
while (-- argc > 0)
printf("%s%s", * ++argv, (argc > 1) ? " " : " ");
printf("\n");
printf((argc > 1) ? "%s " : "%s", *++argv);
return 0;
}
